{
  "name": "mc-icons",
  "version": "1.0.1",
  "description": "Parsing markdown file to replace item names into item icons.",
  "main": "build/index.js",
  "bin": "build/index.js",
  "repository": "https://github.com/Krutoy242/E2E-E-icons",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "tsc",
    "preparse": "ts-node src/preparse.ts"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@committed/trie-search": "^0.1.0",
    "chalk": "^4.1.2",
    "fs-extra": "^10.0.1",
    "lodash": "^4.17.21",
    "mc-iexporter-iterator": "^1.0.0",
    "terminal-kit": "^2.4.0",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@types/fast-levenshtein": "^0.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.180",
    "@types/terminal-kit": "^1.28.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "files": [
    "build",
    "README.md"
  ],
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 120,
    "htmlWhitespaceSensitivity": "ignore"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "env": {
      "es6": true
    },
    "ignorePatterns": [
      "node_modules",
      "build"
    ],
    "plugins": [
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:eslint-comments/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:import/typescript",
      "prettier"
    ],
    "globals": {
      "BigInt": true,
      "console": true,
      "WebAssembly": true
    },
    "rules": {
      "prettier/prettier": 2
    }
  }
}
